<odoo>
    <record id="view_form_habin_orders" model="ir.ui.view">
        <field name="name">habin.orders.form</field>
        <field name="model">habin.orders</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="guest_id"/>
                        <field name="created_at" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_habin_orders" model="ir.ui.view">
        <field name="name">habin.orders.tree</field>
        <field name="model">habin.orders</field>
        <field name="arch" type="xml">
            <tree>
                <field name="guest_id"/>
                <field name="created_at"/>
            </tree>
        </field>
    </record>
</odoo>

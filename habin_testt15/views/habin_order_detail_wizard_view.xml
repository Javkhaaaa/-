<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_habin_order_detail_report_wizard_form" model="ir.ui.view">
    <field name="name">habin.order.detail.report.wizard.form</field>
    <field name="model">habin.order.detail.report.wizard</field>
    <field name="arch" type="xml">
      <form string="Guest Order Detail Report">
        <group>
          <field name="guest_id"/>
        </group>
        <group string="Order Details">
          <field name="order_detail_ids">
            <tree>
              <field name="order_id"/>
              <field name="listing_id"/>
              <field name="check_in_date"/>
              <field name="check_out_date"/>
              <field name="total_days"/>
              <field name="total_price"/>
            </tree>
          </field>
        </group>
        <footer>
          <button string="Close" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
